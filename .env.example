# ============================================================
# SkillLite 快速开始（仅 必需 + 常用 变量）
# cp .env.example .env 并填入配置即可运行
# 完整变量见 .env.example.full，说明见 docs/zh/ENV_REFERENCE.md
# ============================================================

# [必需] LLM API
BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
API_KEY=your-api-key-here
MODEL=qwen3-max

# [常用] Sandbox
ENABLE_SANDBOX=true
SKILLBOX_SANDBOX_LEVEL=3
# 1=无沙箱 2=宽松 3=扫描+确认(默认)

# [常用] Skills 与执行
SKILLS_DIR=./.skills
ALLOW_NETWORK=True
EXECUTION_TIMEOUT=120
